
// This shows a simple example of how to archive the build output artifacts.
pipeline {
        agent any
        tools {nodejs "NODEJS"}


stages {
        stage('install') {
            steps {
                 sh "npm install --force"
            }
        }
         stage("Build"){
           steps{
                sh "npm run ng build"
           }
        }
          stage("Deploy"){
           steps{
                sh "xcopy "C:\ProgramData\Jenkins\.jenkins\workspace\Pipeline\dist\dashboard_seremwe" "C:\Projects\Telone_Aggregator_insurance\QA_ENVI\pipe""
               
           }
        }
    }
}
