
// This shows a simple example of how to archive the build output artifacts.
pipeline {
        agent any
        tools {nodejs "NODEJS"}


stages {
        stage('install') {
            steps {
                 sh "npm install --force"
            }
        }
         stage("Build"){
           steps{
                sh "npm run ng build"
           }
        }
          stage("Deploy"){
           steps{
                bat '''xcopy /s /e /t "C:/ProgramData/Jenkins/.jenkins/workspace/Pipeline@2/dist/dashboard_seremwe" "C:/Projects/pipe"'''
           }
        }
    }
}
